<?xml version="1.0" encoding="UTF-8"?><smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd" xmlns:ftl="http://www.milyn.org/xsd/smooks/freemarker-1.1.xsd" xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.2.xsd">
  <params>
    <param name="inputType">input.xml</param>
    <param name="input.xml" type="input.type.actived">Workspace://jbpmmigration/src/test/resources/jpdl3/singleNode/processdefinition.xml</param>
    <param name="stream.filter.type">DOM</param>
  </params>
  <jb:bean beanId="tdefinitions" class="org.jbpm.bpmn_2_0.TDefinitions" createOnElement="process-definition">
    <jb:value data="/process-definition/@name" property="id"/>
    <jb:value data="/process-definition/@xmlns" property="name"/>
  </jb:bean>
  <jb:bean beanId="startevent" class="org.jbpm.bpmn_2_0.TStartEvent" createOnElement="/process-definition/start-state">
    <jb:value data="/process-definition/start-state/@name" property="id"/>
  </jb:bean>
  <jb:bean beanId="flow1" class="org.jbpm.bpmn_2_0.TSequenceFlow" createOnElement="/process-definition/start-state/transition">
    <jb:value data="/process-definition/start-state/@name" property="id"/>
    <jb:value data="/process-definition/start-state/@name" property="name"/>
    <jb:wiring beanIdRef="sourceRef" property="sourceRef"/>
    <jb:wiring beanIdRef="targetRef" property="targetRef"/>
  </jb:bean>
  <jb:bean beanId="sourceRef" class="java.lang.Object" createOnElement="/process-definition/start-state/@name"/>
  <jb:bean beanId="targetRef" class="java.lang.Object" createOnElement="/process-definition/start-state/transition/@to"/>
  <jb:bean beanId="nodescripttask" class="org.jbpm.bpmn_2_0.TScriptTask" createOnElement="/process-definition/node">
    <jb:value data="/process-definition/node/@name" property="id"/>
    <jb:value data="/process-definition/node/@name" property="name"/>
  </jb:bean>
  <jb:bean beanId="flow2" class="org.jbpm.bpmn_2_0.TSequenceFlow" createOnElement="/process-definition/node/transition">
    <jb:value data="/process-definition/node/@name" property="id"/>
    <jb:value data="/process-definition/node/@name" property="name"/>
    <jb:wiring beanIdRef="sourceRef1" property="sourceRef"/>
    <jb:wiring beanIdRef="targetRef1" property="targetRef"/>
  </jb:bean>
  <jb:bean beanId="sourceRef1" class="java.lang.Object" createOnElement="/process-definition/node/@name"/>
  <jb:bean beanId="targetRef1" class="java.lang.Object" createOnElement="/process-definition/node/transition/@to"/>
  <jb:bean beanId="endstate" class="org.jbpm.bpmn_2_0.TEndEvent" createOnElement="/process-definition/end-state">
    <jb:value data="/process-definition/end-state/@name" property="id"/>
    <jb:value data="/process-definition/end-state/@name" property="name"/>
  </jb:bean>
</smooks-resource-list>
